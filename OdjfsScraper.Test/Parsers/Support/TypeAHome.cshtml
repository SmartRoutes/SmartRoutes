@inherits Parsers.Support.DetailedChildCareTemplate
<div id="PageContent">
    <table>
        <tr><td>Number:                   @Model.ExternalId</td></tr>
        <tr><td>Name:                     @Model.Name</td></tr>
        <tr><td>Address:                  @Model.Address</td></tr>
        <tr><td>City:                     @Model.City</td></tr>
        <tr><td>State:                    @Model.State</td></tr>
        <tr><td>Zip:                      @Model.ZipCode</td></tr>
        <tr><td>Center Status:            @Model.CenterStatus</td></tr>
        <tr><td>Administrators:           @Model.Administrators</td></tr>
        <tr><td>Provider Agreement:       @Model.ProviderAgreement</td></tr>
        <tr><td>County:                   @Model.County.Name</td></tr>
        <tr><td>Initial Application Date: @Model.InitialApplicationDate</td></tr>
        <tr><td>License Begin Date:       @Model.LicenseBeginDate</td></tr>
        <tr><td>License Expiration Date:  @Model.LicenseExpirationDate</td></tr>
        <tr><td>SUTQ Rating:              @SutqRatingToString(Model.SutqRating)</td></tr>
        <tr><td>Phone:                    @Model.PhoneNumber</td></tr>
    </table>
</div>

<table id="providerinfo">
    <tr>
        <td>@CheckBooleanToString(Model.Infants)</td><td>Infants</td>
        <td>@CheckBooleanToString(Model.Naeyc)</td><td>NAEYC</td>
        <td>Monday:</td><td>@DayTimesToString(Model.MondayReported, Model.MondayBegin, Model.MondayEnd)</td>
    </tr>
    <tr>
        <td>@CheckBooleanToString(Model.YoungToddlers)</td><td>Younger Toddler</td>
        <td>@CheckBooleanToString(Model.Necpa)</td><td>NECPA</td>
        <td>Tuesday:</td><td>@DayTimesToString(Model.TuesdayReported, Model.TuesdayBegin, Model.TuesdayEnd)</td>
    </tr>
    <tr>
        <td>@CheckBooleanToString(Model.OlderToddlers)</td><td>Older Toddler</td>
        <td>@CheckBooleanToString(Model.Naccp)</td><td>NACCP</td>
        <td>Wednesday:</td><td>@DayTimesToString(Model.WednesdayReported, Model.WednesdayBegin, Model.WednesdayEnd)</td>
    </tr>
    <tr>
        <td>@CheckBooleanToString(Model.Preschoolers)</td><td>Preschool</td>
        <td>@CheckBooleanToString(Model.Nafcc)</td><td>NAFCC</td>
        <td>Thursday:</td><td>@DayTimesToString(Model.ThursdayReported, Model.ThursdayBegin, Model.ThursdayEnd)</td>
    </tr>
    <tr>
        <td>@CheckBooleanToString(Model.Gradeschoolers)</td><td>School Age</td>
        <td>@CheckBooleanToString(Model.Coa)</td><td>COA</td>
        <td>Friday:</td><td>@DayTimesToString(Model.FridayReported, Model.FridayBegin, Model.FridayEnd)</td>
    </tr>
    <tr>
        <td>@CheckBooleanToString(Model.ChildCareFoodProgram)</td><td>Child Care</td>
        <td>@CheckBooleanToString(Model.Acsi)</td><td>ACSI</td>
        <td>Saturday:</td><td>@DayTimesToString(Model.SaturdayReported, Model.SaturdayBegin, Model.SaturdayEnd)</td>
    </tr>
    <tr>
        <td>Sunday:</td><td>@DayTimesToString(Model.SundayReported, Model.SundayBegin, Model.SundayEnd)</td>
    </tr>
</table>