@inherits RazorEngine.Templating.TemplateBase<SmartRoutes.Model.Odjfs.ChildCares.TypeBHome> 
<div id="PageContent">
    <table>
        <tr><td>Number:                        @Model.ExternalId</td></tr>
        <tr><td>Name:                          @Model.Name</td></tr>
        <tr><td>Address:                       @Model.Address</td></tr>
        <tr><td>City:                          @Model.City</td></tr>
        <tr><td>State:                         @Model.State</td></tr>
        <tr><td>Zip:                           @Model.ZipCode</td></tr>
        <tr><td>County:                        @Model.County</td></tr>
        <tr><td>Certification Begin Date:      @Model.CertificationBeginDate</td></tr>
        <tr><td>Certification Expiration Date: @Model.CertificationExpirationDate</td></tr>
        <tr><td>Phone:                         @Model.PhoneNumber</td></tr>
    </table>
</div>